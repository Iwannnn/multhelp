/**app.wxss**/

@import 'style/wxbootstrap';
@import 'style/iconfont';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.divLine{
  background: #E0E3DA;
  margin:auto;
  width: 94%;
  height: 1rpx;
}


.btn{
  border: 2px solid #f6d5f9; 
  border-radius: 5px;  
  background-color: white;
  font-size: 13px;
  color:#f6d5f9;
}

.btn::after{
  border: none; 
}

.btn-big{
  border: none; 
  border-radius: 5px;  
  background-color: #f6d5f9;
  font-size: 20px;
  color:#ffffff;
  font-weight: 500;
  width: 800;
}

.btn-big::after{
  border: none; 
}